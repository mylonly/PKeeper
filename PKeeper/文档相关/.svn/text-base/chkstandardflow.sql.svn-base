INSERT INTO `chkstandardflow` VALUES (1, 1, '清理鸡场环境，订购雏鸡:如果是公司加农户的养殖模式，需向所在公司订购雏鸡。要求对鸡场内道路，院落进行清扫，清理排水沟，做到排水通畅。打扫舍外环境，清除鸡舍四周的污物，清除杂草，清扫院落，做到场内无鸡粪、羽毛、垃圾，同时也应清理鸡场外环境。从鸡舍、鸡场清理出的脏物应进行无害化处理。', 1);
INSERT INTO `chkstandardflow` VALUES (2, 1, '清理鸡舍，器具清洗消毒:彻底清除舍内的残余饲料、粪便、垫料、羽毛、灰尘等，注意将顶棚、墙壁、地面和门窗清扫干净。清理的设备包括料桶、饮水器、塑料网、温度计、火炉、工作服、水桶等。', 2);
INSERT INTO `chkstandardflow` VALUES (3, 1, '冲洗鸡舍:冲洗顶棚、墙壁、窗户和地面，应做到无粪便、灰尘等脏污有机物残留，地面和1米以下的墙面应该边冲边刷。冲洗饲养设备，包括料桶、饮水器、塑料网、温度计、火炉、工作服、水桶等。舍内任何表面都要冲洗到无脏物附着。冲洗干净是消毒药使用有效的基础。冲洗工作应从上到下，从里到外认真进行。', 3);
INSERT INTO `chkstandardflow` VALUES (4, 1, '检修设备:维修饲养设备，设备应能保证完成饲养一批鸡，否则就要更换。检修供热、通风及照明等设备。', 4);
INSERT INTO `chkstandardflow` VALUES (5, 1, '室外消毒:清扫道路、院落。舍外和整个场区以及场外的道路可用3%的热火碱水消毒或直接撒生石灰。地表过脏的地方在消毒后将其翻到地下。消毒液要喷洒到每个角落。', 5);
INSERT INTO `chkstandardflow` VALUES (6, 1, '鸡舍消毒:对干燥了的鸡舍进行第一次消毒。同时将在舍外洗刷消毒干净的饲养设备搬入鸡舍。地面和1米以下的墙面用3%的火碱消毒，顶棚和四壁可用次氯酸钠等消毒药喷雾消毒。', 6);
INSERT INTO `chkstandardflow` VALUES (7, 1, '安装设备:安装棚架、塑料网和护网，挂好温度计和湿度计。育雏网和育成网应分别安装，温度计和湿度计离床面和地面5厘米。网架表面应无毛刺。人员需经消毒、换好干净的衣服和鞋后再进入消毒好的环境，不能因为进出时的疏漏破坏消毒效果。', 7);
INSERT INTO `chkstandardflow` VALUES (8, 1, '安装设备:地面平养的鸡舍铺设垫料，垫料厚度6～10厘米。摆放饮水器、开食盘，80～100只雏鸡一个开食盘，50只鸡一个饮水器。安装采暖或降温设备。', 8);
INSERT INTO `chkstandardflow` VALUES (9, 1, '二次消毒:用百毒杀等消毒液从上至下对整个鸡舍和器具进行喷洒。墙壁用生石灰乳浆粉刷一遍，地面仍用2%火碱消毒。检查门窗、通风口及屋顶等处，确保没有上批鸡留下的灰尘。地面干燥后，搬进垫料。', 9);
INSERT INTO `chkstandardflow` VALUES (10, 1, '熏蒸消毒:关闭门窗和通风孔，检查有无漏气处，检查温度和湿度。为提高熏蒸消毒效果，尽可能地使舍温达到24℃以上，相对湿度达到75%以上。用药量按每立方米空间42毫升福尔马林、高锰酸钾21克计。舍内每隔10米放一个熏蒸盆，先放入高锰酸钾，然后从距舍门最远端开始依次倒入福尔马林，速度要快，以防呛人，出门后立即把门封严。', 10);
INSERT INTO `chkstandardflow` VALUES (11, 1, '开窗换气:熏蒸24小时后打开门窗、通气口和排气扇，充分换气。注意：人员进出的区域必须消毒、更换干净的衣服和鞋，搬入的物品也必须是干净的。每舍门口设消毒池（盆）。', 11);
INSERT INTO `chkstandardflow` VALUES (12, 1, '关闭门窗:准备和检查落实进雏前的一切准备工作，包括保温措施、饲料、药品疫苗、煤（天然气）、供电及电路等。', 12);
INSERT INTO `chkstandardflow` VALUES (13, 1, '查漏补缺:落实进雏、进料、药品储备，发现不足，及时弥补。', 13);
INSERT INTO `chkstandardflow` VALUES (14, 1, '育雏室设置和预温:用塑料布隔出鸡舍1/3作为育雏室，塑料布至少两层。10日龄内棚架上铺上报纸。1～5天育雏密度每平方米35～40只。冬春季节鸡舍开始预热升温，注意检查炉子是否能正常使用，有无漏烟、倒烟现象，有无火灾隐患。', 14);
INSERT INTO `chkstandardflow` VALUES (15, 1, '预温:开始预热，使舍温和育雏器温度达到要求，育雏温度一般35℃，湿度65%～75%，准备好记录表格及其他工具，准备好雏鸡料、疫苗、疫苗滴管和免疫人员。', 15);
INSERT INTO `chkstandardflow` VALUES (16, 2, '免疫、开饮和开食。在进雏前2小时将饮水器装满20℃左右的温开水，水中可加5%的葡萄糖、适量多维，对运输距离较远或存放时间太长的雏鸡，饮水中还需加适量的补液盐。添水量按每只鸡6毫升计。鸡新城疫及传染性支气管炎免疫，用稀释液稀释点眼，半小时内必须用完，1头份/只，注意应等疫苗进入眼内后才能将鸡放开。饮水后3小时给料。进雏后，边清点雏鸡，边将雏鸡安置在育雏器内休息。待雏鸡开始活动后，先教雏鸡饮水，每百只鸡抓5只，将喙按入水中，1秒左右后松开。雏鸡饮水2～3小时后，开始喂料，轻敲料盘，将饲料撤到垫纸上，少给勤添，每2小时喂1次料。第一次喂料为每只鸡20分钟吃完0.5克为度，日采食量10克/只，以后逐渐增加。实行24小时光照制度，需昼夜有人值班。', 1);
INSERT INTO `chkstandardflow` VALUES (17, 2, ' 注意观察雏鸡的动态、采食情况，注意舍内的温度、通风状况和湿度，注意通风，24小时光照。温度35℃，湿度65%～75%，饮水器每天清洗3次以上，饲料少给勤添，日采食量14克/只，喂料时注意捡出没学会饮水采食的雏鸡，放在适宜的环境中设法调教，挑出弱雏、病雏及时淘汰。', 2);
INSERT INTO `chkstandardflow` VALUES (18, 2, '日采食量18克/只；逐步降低温度，调整温度至34℃，湿度60%～65%；从即日起，调整光照时间，固定为23小时，夜间熄灯1小时。注意清粪。注意观察粪便状况，粪便在报纸上的水圈过大，是雏鸡受凉的标志。发现雏鸡有腹泻时，应该立即从环境控制、卫生管理和用药上采取相应措施。', 3);
INSERT INTO `chkstandardflow` VALUES (19, 2, '天日采食量22克/只；逐步降低温度，调整温度至34℃，湿度60%～65%；撤走1/3小饮水器，放置自动饮水器。注意填写好工作记录。', 4);
INSERT INTO `chkstandardflow` VALUES (20, 2, '日采食量26克/只；逐步降低温度，调整温度为33℃，湿度60%～65%。注意观察鸡群的采食、饮水、呼吸及粪便状况。撤走全部小饮水器，放置自动饮水器。注意鸡舍内环境的稳定。', 5);
INSERT INTO `chkstandardflow` VALUES (21, 2, '日采食量29克/只；调整温度为33℃，湿度60%～65%。撤走1/3开食盘，每50只鸡提供1个料桶。', 6);
INSERT INTO `chkstandardflow` VALUES (22, 2, '日采食量32克/只；调整温度为32℃，湿度60%～65%。疫苗接种，扩大育雏面积，称重。撤走1/3开食盘，每50只鸡提供1个料桶。新城疫滴鼻点眼免疫。饲养面积扩大20%。周末称重：午后2时，抽样2%或100只鸡称重。为使称重的鸡具有代表性，让鸡群活动开后，从5个以上点随机取样，逐只称重。计算鸡群的平均体重和均匀度，检查总结1周内的管理工作。', 7);
INSERT INTO `chkstandardflow` VALUES (23, 2, '日采食量35克/只；调整温度为32℃，湿度60%～65%，饮水中添加多种维生素，撤走1/3开食盘，每50只鸡提供1个料桶。调整好饮水器及料桶的高度，料桶上沿应始终与鸡胸高度相同，饮水器上沿与鸡背高度相同。', 8);
INSERT INTO `chkstandardflow` VALUES (24, 2, '日采食量38克/只；调整温度为31℃，湿度60%～65%，注意通风。', 9);
INSERT INTO `chkstandardflow` VALUES (25, 2, '日采食量41克/只；调整温度为31℃，湿度60%～65%，注意通风。开始饮水消毒，温暖季节饮水器中可以直接添加凉水，水中按说明比例添加百毒杀等消毒药，注意消毒药的比例一定要正确。舍内隔日带鸡喷雾消毒1次，消毒液用量为每平方米35毫升，浓度按消毒药说明书配制。', 10);
INSERT INTO `chkstandardflow` VALUES (26, 2, '日采食量47克/只；调整温度为30℃，湿度60%～65%。', 11);
INSERT INTO `chkstandardflow` VALUES (27, 2, '日采食量49克/只；调整温度为29℃，湿度60%～65%。', 12);
INSERT INTO `chkstandardflow` VALUES (28, 2, '日采食量51克/只；调整温度为29℃，停止饮水消毒。饮水中添加多种维生素，调整料桶、饮水器高度，注意通风。夜间熄灯后仔细倾听鸡群内有无异常呼吸音。', 13);
INSERT INTO `chkstandardflow` VALUES (29, 2, '疫苗接种，扩大育雏面积，称重。饮水中添加多种维生素。疫苗饮水免疫前，停水，一般夏、秋季2～3小时，冬季3～4小时，每只免疫水量为20～25毫升，并且在半小时内饮完。为延长疫苗在饮水中的存活时间，在水中加入0.3%的脱脂奶粉，以每只鸡1头份的量将疫苗溶入水中。同时使用法氏囊疫苗D78饮水免疫。注意观察雏鸡有无接种疫苗后的副反应，如果有精神状态不良等反应时，应该将舍温提高1℃左右。扩大饲养面积20%，日采食量为53克/只，温度29℃，湿度55%～60%。按7日龄时的方法采样称重。根据平均体重和鸡群均匀度分析鸡群的管理状况。', 14);
INSERT INTO `chkstandardflow` VALUES (30, 2, '日采食量57克/只；温度29℃，饮水中加多种维生素，注意通风。彻底清扫舍外环境，用3%火碱消毒。', 15);
INSERT INTO `chkstandardflow` VALUES (31, 2, '日采食量60克/只；温度28℃，开始饮水消毒。夜间熄灯后仔细倾听鸡群内有无异常呼吸音。', 16);
INSERT INTO `chkstandardflow` VALUES (32, 2, '常规管理，注意通风。温度28℃，日采食量64～71克/只；准备2号肉鸡料。注意观察鸡群有无呼吸道症状、神经症状、不正常的粪便。继续隔日带鸡消毒，加强通风换气和环境管理。', 17);
INSERT INTO `chkstandardflow` VALUES (33, 2, '常规管理，注意通风。温度28℃，日采食量64～71克/只；准备2号肉鸡料。注意观察鸡群有无呼吸道症状、神经症状、不正常的粪便。继续隔日带鸡消毒，加强通风换气和环境管理。', 18);
INSERT INTO `chkstandardflow` VALUES (34, 2, '常规管理，注意通风。温度28℃，日采食量64～71克/只；准备2号肉鸡料。注意观察鸡群有无呼吸道症状、神经症状、不正常的粪便。继续隔日带鸡消毒，加强通风换气和环境管理。', 19);
INSERT INTO `chkstandardflow` VALUES (35, 2, '常规管理。温度27℃，日采食量74克/只；饲料中混加1/3的2号肉鸡料，准备扩群。', 20);
INSERT INTO `chkstandardflow` VALUES (36, 2, '常规管理。温度26℃，日采食量78克/只；饲料中混加2/3的2号肉鸡料，扩群至全鸡舍。按前面的方法称重，饮水中增加多种维生素。在控制温度的基础上加强通风换气。对舍外环境清理消毒。', 21);
INSERT INTO `chkstandardflow` VALUES (37, 2, '进行新-支二联苗免疫。按前面饮水免疫的方法进行，注意免疫用水不得含有消毒药，水中加0.3%的脱脂奶粉，水量35～42毫升。温度26℃，日采食量82克/只；从即日起全部改用2号料。', 22);
INSERT INTO `chkstandardflow` VALUES (38, 2, '常规管理。温度26℃，日采食量92克/只；饮水中增加多种维生素。在控制温度的基础上加强通风换气。调整料桶高度。', 23);
INSERT INTO `chkstandardflow` VALUES (39, 2, '常规管理。温度26℃，日采食量97～102克/只；继续隔日带鸡消毒，加强通风换气和环境管理。开始使用纯中药制剂主要是抗大肠杆菌等作用好的饮水剂以及对非典型性新城疫有一定预防和治疗作用的散剂或饮水剂。', 24);
INSERT INTO `chkstandardflow` VALUES (40, 2, '常规管理。温度26℃，日采食量97～102克/只；继续隔日带鸡消毒，加强通风换气和环境管理。开始使用纯中药制剂主要是抗大肠杆菌等作用好的饮水剂以及对非典型性新城疫有一定预防和治疗作用的散剂或饮水剂，连用3～5天。', 25);
INSERT INTO `chkstandardflow` VALUES (41, 2, '常规管理。温度26℃，日采食量97～102克/只；继续隔日带鸡消毒，加强通风换气和环境管理。开始使用纯中药制剂主要是抗大肠杆菌等作用好的饮水剂以及对非典型性新城疫有一定预防和治疗作用的散剂或饮水剂，连用3～5天。', 26);
INSERT INTO `chkstandardflow` VALUES (42, 2, '常规管理。温度25℃，日采食量102克/只；在控制温度的基础上加强通风换气。对舍内外环境清理消毒。停止饮水消毒，饮水中增加多种维生素。', 27);
INSERT INTO `chkstandardflow` VALUES (43, 2, '温度25℃，日采食量107克/只；新城疫活苗饮水免疫，每只鸡2头份。清晨喂料前停水，夏季停水1.5～3.5小时，冬季停水3.5～4小时。水量为当日采食量的40%左右。要求让每只鸡都喝到疫苗，并在半小时内喝净。当日水中加复合多种维生素，不得加消毒药。称重。', 28);
INSERT INTO `chkstandardflow` VALUES (44, 2, '常规管理。温度从即日起稳定在24℃，日采食量112克/只；饮水中增加多种维生素。调整料桶及饮水器高度。日常管理转为以通风换气为主。继续隔日带鸡消毒。本周是鸡群容易发生疾病的阶段，要注意观察鸡群有无神经症状、呼吸道症状及粪便异常，注意鸡群的腿病情况。', 29);
INSERT INTO `chkstandardflow` VALUES (45, 2, '常规管理。日采食量118克/只；开始饮水消毒。', 30);
INSERT INTO `chkstandardflow` VALUES (46, 2, '常规管理。日采食量122～134克/只；对舍内外环境清理消毒。', 31);
INSERT INTO `chkstandardflow` VALUES (47, 2, '常规管理。日采食量122～134克/只；对舍内外环境清理消毒。', 32);
INSERT INTO `chkstandardflow` VALUES (48, 2, '常规管理。日采食量122～134克/只；对舍内外环境清理消毒。', 33);
INSERT INTO `chkstandardflow` VALUES (49, 2, '常规管理。日采食量122～134克/只；对舍内外环境清理消毒。', 34);
INSERT INTO `chkstandardflow` VALUES (50, 2, '常规管理。隔日带鸡消毒。称重，比较与标准体重的差距，分别计算公、母鸡的均匀度。停止饮水消毒。使用纯中药制剂主要是抗大肠杆菌等作用好的饮水剂以及对非典型性新城疫有一定预防和治疗作用的散剂或饮水剂，连用3～5天。加强通风换气。', 35);
INSERT INTO `chkstandardflow` VALUES (51, 2, '常规管理。日采食量139～162克/只；按日常管理注意事项工作。继续隔日带鸡消毒。以加强通风换气、维持舍内宁静舒适的环境为工作重心。注意秋、冬季节的昼夜温差，控制好舍温。夏季则需注意防暑工作。准备3号料。', 36);
INSERT INTO `chkstandardflow` VALUES (52, 2, '常规管理。日采食量139～162克/只；按日常管理注意事项工作。继续隔日带鸡消毒。以加强通风换气、维持舍内宁静舒适的环境为工作重心。注意秋、冬季节的昼夜温差，控制好舍温。夏季则需注意防暑工作。准备3号料。', 37);
INSERT INTO `chkstandardflow` VALUES (53, 2, '常规管理。日采食量139～162克/只；按日常管理注意事项工作。继续隔日带鸡消毒。以加强通风换气、维持舍内宁静舒适的环境为工作重心。注意秋、冬季节的昼夜温差，控制好舍温。夏季则需注意防暑工作。准备3号料。', 38);
INSERT INTO `chkstandardflow` VALUES (54, 2, '常规管理。日采食量139～162克/只；按日常管理注意事项工作。继续隔日带鸡消毒。以加强通风换气、维持舍内宁静舒适的环境为工作重心。注意秋、冬季节的昼夜温差，控制好舍温。夏季则需注意防暑工作。准备3号料。', 39);
